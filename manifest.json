{
	"manifest_version": 2,
 	"name": "Email Assistant",
 	"version": "0.1",
 	"permissions": [
	    "https://mail.google.com/",
	    "https://inbox.google.com/"
  	],
 	"content_scripts": [{
	    	"matches": [
	    	  	"https://mail.google.com/*", "http://mail.google.com/*"
	    	],
	    	"js": ["inboxsdk.js", "tracker.js","jquery.js"]
	}]
}

